{
  "type": "object",
  "$id": "schema:user:userProfile",
  "required": ["username", "email"],
  "properties": {
    "username": { "type": "string" },
    "email": { "type": "string", "format": "email" },
    "nickname": { "type": "string" },
    "created": { "type": "string", "format": "date-time" },
    "avatar": {
      "type": "object",
      "properties": {
        "url": { "type": "string", "format": "uri" }
      },
      "required": ["url"]
    }
  }
}