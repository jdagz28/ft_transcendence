{
  "type": "object",
  "$id": "schema:users:userProfile",
  "required": [
    "id",
    "username",
    "email",
    "nickname",
    "created",
    "avatar"
  ],
  "properties": {
    "id": { "type": "number" },
    "username": { "type": "string" },
    "email": { "type": "string", "format": "email" },
    "nickname": { "type": "string" },
    "created": { "type": "string", "format": "date-time" },
    "avatar": {
      "type": "object",
      "properties": {
        "url": { "type": "string", "format": "uri" }
      },
      "required": ["url"]
    }
  }
}